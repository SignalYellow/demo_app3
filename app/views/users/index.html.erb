<% provide(:title, 'All users') %>
<h1><%= @msg %></h1>
<div class="span6 offset3">
  
<%= form_tag("/users", method: "get") do %>
  <%= label_tag(:q, "Search for:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>
</div>
<div class="span10 offset1">
<%= will_paginate %>

<ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>
</div>